{
  "name": "express-marshal",
  "version": "1.1.2",
  "description": "Helpful decorators to wire up Express controllers",
  "license": "MIT",
  "repository": "heydoctor/express-marshal",
  "author": {
    "name": "Kyle Alwyn",
    "email": "kjalwyn@gmail.com",
    "url": "https://kylealwyn.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier --single-quote --trailing-comma es5 --write '{src,test}/**/*.ts'",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "release": "npm run build && standard-version"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "express",
    "decorators",
    "es2015"
  ],
  "dependencies": {
    "express": "^4.16.4",
    "joi": "^14.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.0.0",
    "@types/express": "^4.17.5",
    "@types/jest": "^24.0.18",
    "@types/joi": "^14.3.3",
    "@types/supertest": "^2.0.8",
    "body-parser": "^1.18.3",
    "jest": "^24.9.0",
    "prettier": "^1.16.4",
    "rimraf": "^3.0.0",
    "standard-version": "^6.0.1",
    "supertest": "^4.0.2",
    "typescript": "^3.6.3"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*"
    ]
  }
}
